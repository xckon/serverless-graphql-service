{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "env": {
    "INSTROSPECTION": "true",
    "PLAYGROUND": "true",
    "PORT": "80",
    "MY_AWS_ACCESS_KEY_ID": "@dynamodb-aws-access-key-id",
    "MY_AWS_SECRET_ACCESS_KEY": "@dynamodb-aws-secret-access-key",
    "MY_AWS_DEFAULT_REGION": "us-west-2",
    "SECRET": "@jwt-secret",
    "SECURE_USERNAME": "@secure-username",
    "SECURE_PASSWORD": "@secure-password"
  }
}
